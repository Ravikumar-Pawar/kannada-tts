{
    "model": "tacotron2",
    "run_name": "kannada_tts_baseline",
    "epochs": 500,
    "batch_size": 16,
    "eval_batch_size": 8,
    "num_loader_workers": 4,
    "run_eval": true,
    "epochs_per_eval": 10,
    "save_step": 1000,
    "save_n_checkpoints": 3,
    "lr": 0.001,
    "lr_scheduler": "Noam",
    "lr_scheduler_params": {
        "warmup_steps": 4000
    },
    "characters": {
        "pad": "_",
        "eos": "~",
        "bos": "^",
        "blank": " ",
        "characters": "!'.(),-.:;?ಅಆಇಈಉಊಋಎಏಐಒಓಔಕಖಗಘಙಚಛಜಝಞಟಠಡಢಣತಥದಧನಪಫಬಭಮಯರಲವಶषಸಹೃೈೊೋೌಂಃೞ"
    },
    "phoneme_language": null,
    "audio": {
        "sample_rate": 22050,
        "hop_length": 256,
        "win_length": 1024,
        "n_mel_channels": 80,
        "mel_fmin": 0,
        "mel_fmax": 8000
    },
    "use_phonemes": false,
    "text_cleaner": "basic_cleaners"
}